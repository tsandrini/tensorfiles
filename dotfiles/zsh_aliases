alias gmake="make"

# Sys
alias startx="ssh-agent startx"

# Custom stuff aliases
alias vi="vim"
alias vim="nvim"
alias shutnow="shutdown now"
alias vtop="vtop --theme wal"
alias ls="exa"
alias ll="exa -F --icons --group-directories-first -la --git --header --created --modified"
alias tree="exa -F --icons --group-directories-first -la --git --header --created --modified -T";
alias cat="bat -p --wrap=never --paging=never"
alias less="bat --paging=always"

alias untarball="tar -xvzf"
alias g="git"

. "${HOME}/.cache/wal/colors.sh"
